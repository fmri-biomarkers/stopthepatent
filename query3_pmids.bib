@article{Pich2012,
mesh_heading_list = "Animals and Antipsychotic Agents and Biomarkers and Clinical Trials as Topic and Glycine and Glycine Plasma Membrane Transport Proteins and Humans and Metabolomics and Precision Medicine and Proteomics and Schizophrenia",
doi = "10.1007/978-3-642-25761-2_13",
isbn = "0171-2004",
title = "Biomarkers for antipsychotic therapies.",
abstract = "Molecular biomarkers for antipsychotic treatments have been conceptually linked to the measurements of dopamine functions, mostly D(2) receptor occupancy, either by imaging using selective PET/SPECT radioactive tracers or by assessing plasma prolactin levels. A quest for novel biomarkers was recently proposed by various academic, health service, and industrial institutions driven by the need for better treatments of psychoses. In this review we conceptualize biomarkers within the Translational Medicine paradigm whose goal was to provide support to critical decision-making in drug discovery. At first we focused on biomarkers as outcome measure of clinical studies by searching into the database clinicaltrial.gov. The results were somewhat disappointing, showing that out of 1,659 antipsychotic trials only 18 used a biomarker as an outcome measure. Several of these trials targeted plasma lipids as sentinel marker for metabolic adverse effects associated with the use of atypical antipsychotics, while only few studies were aimed to new disease specific biological markers. As an example of a mechanistic biomarker, we described the work done to progress the novel class of glycine transporter inhibitors as putative treatment for negative symptoms of schizophrenia. We also review how large-scale multiplex biological assays were applied to samples from tissues of psychiatric patients, so to learn from changes of numerous analytes (metabolic products, lipids, proteins, RNA transcripts) about the substrates involved in the disease. We concluded that a stringent implementation of these techniques could contribute to the endophenotypic characterization of patients, helping in the identification of key biomarkers to drive personalized medicine and new treatment development.",
author = "Pich, Emilio Merlo and Vargas, Gabriel and Domenici, Enrico",
number = "212",
journal_iso = "Handb Exp Pharmacol",
year = "2012",
journal = "Handbook of experimental pharmacology",
pmid = "23129338",
pages = "339-60"
}
@article{Mahlmann2012,
mesh_heading_list = "Adult and Antibodies, Monoclonal and Coronary Angiography and Coronary Artery Bypass and Coronary Artery Disease and Drug Therapy, Combination and Echocardiography, Stress and Female and Fluorodeoxyglucose F18 and Glucocorticoids and Humans and Immunosuppressive Agents and Infliximab and Magnetic Resonance Angiography and Methotrexate and Positron-Emission Tomography and Predictive Value of Tests and Radiopharmaceuticals and Remission Induction and Takayasu Arteritis and Treatment Outcome and Tumor Necrosis Factor-alpha",
doi = "10.1024/0301-1526/a000236",
isbn = "0301-1526",
title = "Combined immunosuppressive therapy including a TNF-alpha blocker induces remission in a difficult to treat patient with Takayasu arteriitis and coronary involvement.",
abstract = "A 40 year old woman presented with symptoms of a systemic inflammatory disease and obstruction of the left subclavian artery. Takayasu arteriitis (TA) was clinically diagnosed and confirmed by MR angiography and FDG-PET scan showing inflammation of the aortic arch and the left subclavian artery. Immunosuppression with glucocorticoids and methotrexate resulted in immediate clinical improvement and normalization of systemic markers of inflammation. Despite that the patient developed chest pain on exertion suggesting coronary involvement, which was confirmed by dobutamine stress echocardiography. After adding the TNF-alpha blocker infliximab coronary symptoms gradually improved and a clinically stable situation could be achieved for more than 6 months. Coronary angiography and aortography showed an occluded main stem of the left coronary artery, an occluded left subclavian artery, and stenoses of the brachiocephalic trunk and the left common carotid artery. Revascularization of the coronary artery and the aortic arch and its branches was performed. The patient returned to work two months after the operation. Immunosuppressive therapy with infliximab and methotrexate is continued, glucocorticoids were stopped after one year of treatment. This case shows that vascular progress in TA patients may occur even when systemic inflammation is controlled, therefore patients have to be carefully observed for new vascular manifestations. TNF-alpha blockers may be an additional treatment option in otherwise difficult to treat TA patients allowing to perform revascularization after a stable disease state has been achieved.",
author = "Mahlmann, A and Pfluecke, C and Ouda, A and Simonis, G and Weiss, N and Kappert, U",
number = "6",
volume = "41",
journal_iso = "VASA",
year = "2012",
journal = "VASA. Zeitschrift für Gefässkrankheiten",
pmid = "23129041",
pages = "451-7"
}
