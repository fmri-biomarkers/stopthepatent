mesh_heading_list = "Aminopeptidases and Animals and Antigens, CD13 and Cell Line, Tumor and Female and Gallium Radioisotopes and Gene Expression and Heterografts and Humans and Mice and Molecular Imaging and Oligopeptides and Positron-Emission Tomography and Protein Binding and Signal-To-Noise Ratio and Tissue Distribution",
doi = "10.3390/molecules190811600",
