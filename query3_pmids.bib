year = "2012",
journal = "Conference proceedings : ... Annual International Conference of the IEEE Engineering in Medicine and Biology Society. IEEE Engineering in Medicine and Biology Society. Annual Conference",
pmid = "23367161",
pages = "5445-8"
}
@article{Sato2012,
