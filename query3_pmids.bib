mesh_heading_list = "Adult and Aged and Aged, 80 and over and Biomarkers and Female and Humans and Magnetic Resonance Imaging and Middle Aged and Multimodal Imaging and Neoplasm Recurrence, Local and Pelvic Exenteration and Positron-Emission Tomography and Preoperative Care and Retrospective Studies and Tomography, X-Ray Computed and Uterine Neoplasms and Vulvar Neoplasms",
doi = "10.1097/IGC.0b013e3182a41e61",
