@article{Sakakibara2014,
mesh_heading_list = "Adult and Algorithms and Brain Diseases and Educational Status and Female and Functional Neuroimaging and Gene-Environment Interaction and Genetics, Behavioral and Hemoglobins and Humans and Intelligence Tests and Male and Mental Disorders and Prefrontal Cortex and Psychomotor Performance and Socioeconomic Factors and Spectroscopy, Near-Infrared and Twins, Dizygotic and Twins, Monozygotic and Verbal Behavior",
doi = "10.1016/j.neuroimage.2013.03.052",
isbn = "1095-9572",
title = "Genetic influences on prefrontal activation during a verbal fluency task in adults: a twin study based on multichannel near-infrared spectroscopy.",
abstract = "Near-infrared spectroscopy (NIRS) studies have reported that prefrontal hemodynamic dysfunction during executive function tasks may be a promising biomarker of psychiatric disorders, because its portability and noninvasiveness allow easy measurements in clinical settings. Here, we investigated the degree to which prefrontal NIRS signals are genetically determined. Using a 52-channel NIRS system, we monitored the oxy-hemoglobin (oxy-Hb) signal changes in 38 adult pairs of right-handed monozygotic (MZ) twins and 13 pairs of same-sex right-handed dizygotic (DZ) twins during a letter version of the verbal fluency task. Heritability was estimated based on a classical twin paradigm using structured equation modeling. Significant genetic influences were estimated in the right dorsolateral prefrontal cortex and left frontal pole. The degrees of heritability were 66% and 75% in the variances, respectively. This implies that the prefrontal hemodynamic dysfunction observed during an executive function task measured by NIRS may be an efficient endophenotype for large-scale imaging genetic studies in psychiatric disorders.",
author = "Sakakibara, Eisuke and Takizawa, Ryu and Nishimura, Yukika and Kawasaki, Shingo and Satomura, Yoshihiro and Kinoshita, Akihide and Koike, Shinsuke and Marumo, Kohei and Kinou, Masaru and Tochigi, Mamoru and Nishida, Nao and Tokunaga, Katsushi and Eguchi, Satoshi and Yamasaki, Syudo and Natsubori, Tatsunobu and Iwashiro, Norichika and Inoue, Hideyuki and Takano, Yosuke and Takei, Kunio and Suga, Motomu and Yamasue, Hidenori and Matsubayashi, Junko and Kohata, Kenji and Shimojo, Chie and Okuhata, Shiho and Kono, Toshiaki and Kuwabara, Hitoshi and Ishii-Takahashi, Ayaka and Kawakubo, Yuki and Kasai, Kiyoto",
