}
@article{Peng2016,
doi = "jnumed.115.161513",
isbn = "1535-5667",
title = "Modulation of abnormal metabolic brain networks by experimental therapies in a nonhuman primate model of Parkinson's disease: an application to human retinal pigment epithelial (hRPE) cell implantation.",
abstract = "Abnormal covariance pattern of regional metabolism associated with Parkinson's disease (PD) is modulated by dopaminergic pharmacotherapy. Using high resolution FDG PET and network analysis we previously derived and validated a parkinsonism-related metabolic pattern (PRP) in non-human primate models of PD. It is currently not known whether this network is modulated by experimental therapeutics. In this study we examined changes in network activity by striatal implantation of human levodopa-producing retinal pigment epithelial (hRPE) cells in parkinsonian macaques and evaluated its reproducibility in a small test-retest study.",
author = "Peng, Shichun and Ma, Yilong and Flores, Joseph and Cornfeldt, Michael and Mitrovic, Branka and Eidelberg, David and Doudet, Doris J",
journal_iso = "J. Nucl. Med.",
