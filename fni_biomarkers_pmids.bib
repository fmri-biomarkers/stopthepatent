journal_iso = "Medicine (Baltimore)",
year = "2015",
journal = "Medicine",
pmid = "26469915",
pages = "e1753"
}
@article{Zmuda2015,
mesh_heading_list = "Animals and Cell Line, Tumor and Enzyme Inhibitors and Glioblastoma and Humans and Iodine Radioisotopes and Mice and Mice, Nude and Phthalazines and Piperazines and Poly(ADP-ribose) Polymerase Inhibitors and Poly(ADP-ribose) Polymerases and Tissue Distribution and Tomography, Emission-Computed, Single-Photon",
