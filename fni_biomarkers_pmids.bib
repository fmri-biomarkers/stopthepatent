@article{Horga2014,
mesh_heading_list = "Acoustic Stimulation and Adult and Auditory Cortex and Brain Mapping and Case-Control Studies and Decision Making and Female and Hallucinations and Humans and Image Processing, Computer-Assisted and Male and Middle Aged and Neuropsychological Tests and Oxygen and Predictive Value of Tests and Schizophrenia and Schizophrenic Psychology and Speech Perception and Time Factors",
doi = "10.1523/JNEUROSCI.0200-14.2014",
isbn = "1529-2401",
title = "Deficits in predictive coding underlie hallucinations in schizophrenia.",
abstract = "The neural mechanisms that produce hallucinations and other psychotic symptoms remain unclear. Previous research suggests that deficits in predictive signals for learning, such as prediction error signals, may underlie psychotic symptoms, but the mechanism by which such deficits produce psychotic symptoms remains to be established. We used model-based fMRI to study sensory prediction errors in human patients with schizophrenia who report daily auditory verbal hallucinations (AVHs) and sociodemographically matched healthy control subjects. We manipulated participants' expectations for hearing speech at different periods within a speech decision-making task. Patients activated a voice-sensitive region of the auditory cortex while they experienced AVHs in the scanner and displayed a concomitant deficit in prediction error signals in a similar portion of auditory cortex. This prediction error deficit correlated strongly with increased activity during silence and with reduced volumes of the auditory cortex, two established neural phenotypes of AVHs. Furthermore, patients with more severe AVHs had more deficient prediction error signals and greater activity during silence within the region of auditory cortex where groups differed, regardless of the severity of psychotic symptoms other than AVHs. Our findings suggest that deficient predictive coding accounts for the resting hyperactivity in sensory cortex that leads to hallucinations.",
author = "Horga, Guillermo and Schatz, Kelly C and Abi-Dargham, Anissa and Peterson, Bradley S",
number = "24",
volume = "34",
journal_iso = "J. Neurosci.",
