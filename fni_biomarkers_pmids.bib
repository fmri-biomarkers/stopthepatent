@article{Hosey2015,
mesh_heading_list = "Aggression and Animal Husbandry and Animal Welfare and Animals and Animals, Zoo and Behavior, Animal and Bonding, Human-Pet and Conservation of Natural Resources and Humans and Models, Psychological",
doi = "10.1002/zoo.21182",
isbn = "1098-2361",
title = "Are we ignoring neutral and negative human-animal relationships in zoos?",
abstract = "Human-animal interactions (HAI), which may lead to human-animal relationships (HAR), may be positive, neutral, or negative in nature. Zoo studies show that visitors may be stressful, may have no effect, or may be enriching. There is also evidence that good HARs set up between animals and their keepers can have positive effects on animal welfare. However, we need to know more about negative HARs, and as a first step we attempt to do this here by considering cases where animals attack people in the zoo. Due to the sensitivity and rarity of these events data appear sparse and unsystematically collected. Here, information available in the public domain about the circumstances of these attacks has been collated to test hypotheses about negative HAIs derived from a model of zoo HARs. The limited data presented here broadly support the zoo HAR model, and suggest that attacks usually happen in unusual circumstances, where there may be a failure by the animal to recognise the HAR, or where the relationship, if there is one, does not hold; and give some support to the prediction that exposure to many keepers may impair the development of a positive HAR. This study may provide useful information for the zoo community to proactively collect systematic standardised records, which will enable a fuller understanding of zoo HARs, upon which appropriate measures might be adopted to build better zoo HARs, which are likely to positively impact zoo animal welfare, and reduce these rare incidences further.",
author = "Hosey, Geoff and Melfi, Vicky",
number = "1",
volume = "34",
journal_iso = "Zoo Biol.",
year = "2015",
journal = "Zoo biology",
pmid = "25328013",
pages = "1-8"
}
@article{Hyman2015,
mesh_heading_list = "Adolescent and Adult and Aged and Amino Acid Substitution and Biopsy and Child and Codon and Cross-Sectional Studies and DNA Mutational Analysis and Erdheim-Chester Disease and Female and Gene Frequency and Genes, ras and Genotype and Histiocytosis and Histiocytosis, Langerhans-Cell and Humans and Magnetic Resonance Imaging and Male and Middle Aged and Mutation and Positron-Emission Tomography and Proto-Oncogene Proteins B-raf and Young Adult",
doi = "10.1158/2159-8290.CD-14-0742",
isbn = "2159-8290",
title = "Prospective blinded study of BRAFV600E mutation detection in cell-free DNA of patients with systemic histiocytic disorders.",
abstract = "Patients with Langerhans cell histiocytosis (LCH) and Erdheim-Chester disease (ECD) have a high frequency of BRAF(V600E) mutations and respond to RAF inhibitors. However, detection of mutations in tissue biopsies is particularly challenging in histiocytoses due to low tumor content and stromal contamination. We applied a droplet-digital PCR assay for quantitative detection of the BRAF(V600E) mutation in plasma and urine cell-free (cf) DNA and performed a prospective, blinded study in 30 patients with ECD/LCH. There was 100% concordance between tissue and urinary cfDNA genotype in treatment-naïve samples. cfDNA analysis facilitated identification of previously undescribed KRAS(G12S)-mutant ECD and dynamically tracked disease burden in patients treated with a variety of therapies. These results indicate that cfDNA BRAF(V600E) mutational analysis in plasma and urine provides a convenient and reliable method of detecting mutational status and can serve as a noninvasive biomarker to monitor response to therapy in LCH and ECD.",
author = "Hyman, David M and Diamond, Eli L and Vibat, Cecile Rose T and Hassaine, Latifa and Poole, Jason C and Patel, Minal and Holley, Veronica R and Cabrilo, Goran and Lu, Timothy T and Arcila, Maria E and Chung, Young Rock and Rampal, Raajit and Lacouture, Mario E and Rosen, Neal and Meric-Bernstam, Funda and Baselga, José and Kurzrock, Razelle and Erlander, Mark G and Janku, Filip and Abdel-Wahab, Omar",
number = "1",
volume = "5",
journal_iso = "Cancer Discov",
year = "2015",
journal = "Cancer discovery",
pmid = "25324352",
pages = "64-71"
}
